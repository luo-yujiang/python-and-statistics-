    "# （1）请用列表录入该数据。\n",
    "a = []\n",
    "f = open(\"3-2.txt\", \"r\",encoding='utf-8')\n",
    "for line in f.readlines():\n",
    "    line=line.strip('\\n')\n",
    "    line=line.split('，')\n",
    "    a.append(line) \n",
    "print(a)\n",
